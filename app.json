{
  "expo": {
    "name": "ARLocationView",
    "slug": "ARLocationView",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-light.png",
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF",
          "imageWidth": 200,
          "dark": {
            "image": "./assets/icons/splash-icon-dark.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to display nearby places in augmented reality.",
        "NSLocationWhenInUseUsageDescription": "We need access to your location to show places around you based on distance."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.engomer.arlocationview"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#FFFFFF",
      "hidden": true
    },
    "web": {
      "favicon": "./assets/icons/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8f39bfea-17fe-4ee6-8700-5e850478cb28"
      }
    }
  }
}