{
  "expo": {
    "name": "ARLocationView",
    "slug": "ARLocationView",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to display nearby places in augmented reality.",
        "NSLocationWhenInUseUsageDescription": "We need access to your location to show places around you based on distance."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF",
        "resizeMode": "contain"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.engomer.arlocationview"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#113295"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "8f39bfea-17fe-4ee6-8700-5e850478cb28"
      }
    }
  }
}